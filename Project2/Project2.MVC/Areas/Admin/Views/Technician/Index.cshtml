@model List<Technician>


<a type="button" class="btn btn-success mb-3" asp-area="Admin" asp-controller="Technician" asp-action="Create">Create Technician</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Position</th>
            <th scope="col">ImagePath</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Technician item in Model) 
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Position</td>
                <td><img src="~/img/@item.ImagePath" /></td>
                <td><a class="btn btn-primary" asp-area="Admin" asp-controller="Technician" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
                <td><a class="btn btn-danger" asp-controller="Technician" asp-action="Delete" asp-route-id="@item.Id" asp-area="Admin">Delete</a></td>
            </tr>
        }
   
    </tbody>
</table>