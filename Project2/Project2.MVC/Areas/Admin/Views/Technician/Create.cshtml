@model TechnicianPostDto;



<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="Name">Name</label>
        <input class="form-control" asp-for="Name">
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="Position">Position</label>
        <input class="form-control" asp-for="Position">
    </div>
    <div class="form-group">
        <label for="ImagePath">ImagePath</label>
        <input class="form-check-input" asp-for="ImagePath">
    </div>
    <div class="form-group">
        <label for="CreatedAt">CreatedAt</label>
        <input class="form-check-input" asp-for="CreatedAt">
    </div>
    <div class="form-group">
        <label for="UpdatedAt">UpdatedAt</label>
        <input class="form-check-input" asp-for="UpdatedAt">
    </div>
    <div class="form-group">
        <label for="DeletedAt">DeletedAt</label>
        <input class="form-check-input" asp-for="DeletedAt">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
